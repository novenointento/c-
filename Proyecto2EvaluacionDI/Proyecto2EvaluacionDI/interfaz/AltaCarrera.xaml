<Window x:Class="Proyecto2EvaluacionDI.interfaz.AltaCarrera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AltaCarrera" Height="177.6" Width="253.6">
    <Grid>
        <Button x:Name="botonAlta" Content="Alta" HorizontalAlignment="Left" Margin="135,97,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <Button x:Name="botonVolver" Content="Volver" HorizontalAlignment="Left" Margin="26,97,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Label Content="Nombre de la carrera:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="33" Width="147"/>
        <TextBox x:Name="textBoxNombre" Validation.Error="validation" Text="{Binding Path=Name, NotifyOnValidationError=True, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Height="23" Margin="10,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="213"/>

    </Grid>
</Window>
